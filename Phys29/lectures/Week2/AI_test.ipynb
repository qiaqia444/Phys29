   "source": [
    "import numpy as np\n",
    "from scipy.integrate import quad\n",
    "\n",
    "# Define the function to integrate\n",
    "def f(x):\n",
    "    return np.sin(x)\n",
    "\n",
    "# Perform the integration\n",
    "result, error = quad(f, 0, 2*np.pi)\n",
    "\n",
    "# Print the result\n",
    "print(\"The integral of sin(x) from 0 to 2pi is:\", result)\n"
   ]
